page {
    height: 100%;
}
.navbar {
    display: flex;
    background: #fff;
    height: 80rpx;
}
.navbar .item {
    position: relative;
    flex: auto;
    text-align: center;
    line-height: 80rpx;
}

.navbar .item.active {
    color: #31c27c;
}
.navbar .item.active:after {
    content: "";
    display: block;
    position: absolute;
    bottom: 0;
    left: 0;
    right: 0;
    height: 4rpx;
    background: #31c27c;
}


.musicPlay{
  position: fixed;
  border-top: 1px solid #eee;
  bottom: 0px;
  left: 0px;
  right: 0px;
  background: #fff; 
  z-index: 9999;
  height: 145rpx; 
}

.song-info{
    flex: none;
    padding: 30rpx;
    display: flex;
    height: 160rpx;
    box-sizing: border-box;
    width: 100%;
    justify-content: space-between;
    align-items: center;
}

.song-info_img{
    flex: 0 0 100rpx;
    margin-right: 30rpx;
    width: 100rpx;
    height: 100rpx;
}

.song-info_con{
    width: 60%;
}

.song-name {
    display: block;
    font-size: 36rpx;
    overflow: hidden;
    text-overflow: ellipsis;
    white-space: nowrap;
}

.singer-name {
    display: block;
    font-size: 28rpx;
    position: relative;
}

.play {
    flex: 0 0 86rpx;
    width: 86rpx;
    height: 86rpx;
    margin-left: 20rpx;
    background: url(data:image/png;base64,) no-repeat;
    background-size: contain;
}

.play.pause {
    background: url(data:image/png;base64,) no-repeat;
    background-size: contain;
}